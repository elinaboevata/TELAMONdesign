                    <lb n="2"/><w lemma="λεγιών"><expan><abbr>λεγ</abbr><ex>ιῶνος</ex></expan></w> <g type="dot"/> <num value="15">ει</num> <g type="dot"/> <placeName>Ἀποληαρίας</placeName> <!--НЕ ЗНАМ КАКВО Е ТОВА-->
