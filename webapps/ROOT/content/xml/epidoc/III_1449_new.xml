            <div type="edition" xml:space="preserve" xml:lang="grc">
                <ab>
                    <lb n="1"/><w lemma="ἀγαθός">ἀγαθῆι</w> <w lemma="τύχη">τύχηι</w>.
                    <lb n="2"/><w lemma="σωτήρ">σωτῆρι</w> <persName type="divine" ref="listDivine.xml#Asclepius"><name>Ἀσκληπιῷ</name></persName> <w>καὶ</w>
                    <lb n="3"/><persName type="divine" ref="listDivine.xml#Hygia"><name>Ὑγείᾳ</name></persName> <w>καὶ</w> <persName type="divine" ref="listDivine.xml#Telesphorus"><name>Τελεσφόρῳ</name></persName><!-- НЯМА ГО В СПИСЪКА -->
                    <lb n="4"/><w lemma="θεός">θεοῖς</w> <w lemma="ἐπήκοος">ἐπηκόοις</w> <persName type="attested" ref="listPers.xml#AT027"><name ref="Onom.xml#Eustochius" nymRef="Εὐστοχίος">Εὐστό
                    <lb n="5" break="no"/>χιος</name> <name ref="Onom.xml#Celer" nymRef="Κέλερ">Κέλερ</name> <name nymRef="Ἀσκληπιάδης" ref="Onom.xml#Asclepiades">Ἀσκληπιάδου</name></persName>
                    <lb n="6"/><w lemma="πρῶτος">πρῶτος</w> <w lemma="ἄρχων">ἄρχων</w> <w lemma="εὐχαριστήριον"><expan><abbr>εὐχαριστήρι</abbr><ex>ον</ex></expan></w>.
<!-- тук сложи гръцкия текст -->
                </ab>
