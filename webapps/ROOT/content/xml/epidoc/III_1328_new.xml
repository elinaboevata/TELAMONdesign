                    <lb n="2"/><w lemma="εἶδος">εἶδος</w> <w lemma="ἀγαθός">ἀρίστης</w></l> <l n="2"><w>καὶ</w> <w lemma="γενεά">γενεὰν</w> <w lemma="παῖς">παίδων</w>
