                    <lb n="7"/><w>μὴ</w> <w lemma="ἕτερος">ἕτερον</w> <w lemma="κατατίθημι">καταθέσθαι</w>· 
                        <w lemma="ἐάν">ἐὰν</w> <w>δέ</w> <w lemma="τις">τις</w> <w lemma="τολμάω">τολμήσει</w>,
                    <lb n="8"/><w lemma="δίδωμι">δώσει</w> <w>τῷ</w> <w lemma="ἱερός">ἱερῷ</w> <w lemma="ταμεῖον">ταμείῳ</w> 
