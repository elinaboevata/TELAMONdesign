            <div type="edition" xml:space="preserve" xml:lang="grc"> 
                <ab>
                     <lb n="1"/> <persName> <name ref="Onom.xml#Aurelia" nymRef="Αὐρηλία"><expan><abbr>Αὐρ</abbr><ex>ηλίαν</ex></expan></name> <g type="leaf"/> <!-- Навсякъде в надписа съм слагал символ за листо, но той е малко по-особен - като арабско "3" ИВ -->
                                        <name nymRef="Τατία" ref="Onom.xml#Tatia">Τατίαν</name> <name nymRef="Λογγεῖνα" ref="Onom.xml#Longina">Λογγεῖναν</name> </persName> <persName> <!-- ИМЕНАТА Τατία И Λογγεῖνα ГИ НЯМА В АУТОРИТИ СПИСЪКА -->
                                        <name ref="Onom.xml#Longinus" nymRef="Λονγεῖνος">Λογγείνου</name></persName> <w lemma="νέα">νέαν</w>
                     <lb n="2"/> <w lemma="ἡρωίς">ἡρωΐδα</w> <g type="leaf"/> <persName ref="listPers.xml#AT323" type="attested"><name ref="Onom.xml#Aurelius" nymRef="Αὐρήλιος">
                                       <expan><abbr>Αὐρ</abbr><ex>ήλιος</ex></expan> </name> <g type="leaf"/> <name ref="Onom.xml#Asclepiodotos" nymRef="Ἀσκληπιόδοτος">Ἀσκληπιόδοτος</name></persName> 
                                       <g type="leaf"/> <persName> <name ref="Onom.xml#Aelius" nymRef="Αἴλιος"><expan><abbr>Αἰλ</abbr><ex>ίου</ex></expan></name> <g type="leaf"/>
                     <lb n="3"/> <name nymRef="Ἰουλιανός" ref="Onom.xml#Iulianus">Ἰουλιανοῦ</name> </persName> <w lemma="βουλευτής">βουλευτὴς</w> <w>καὶ</w> <w lemma="γερουσιαστής">γερουσιαστὴς</w> <!-- Ἰουλιανός ГО НЯМА В СПИСЪКА -->
                     <lb n="4"/> ὁ <w lemma="ἀνὴρ">ἀνὴρ</w> <w lemma="μνεία">μνείας</w> <g type="leaf"/> <w lemma="χάρις">χάριν</w> <g type="leaf"/> 
                                        <w lemma="ζάω"><expan><abbr>ζή</abbr><ex>σασαν</ex></expan></w> <w lemma="ἔτος"><expan><abbr>ἔτ</abbr><ex>η</ex></expan></w> <g type="leaf"/> <num value="18">ιη</num>.
                </ab>
            </div>
            <div type="apparatus">
