                        <persName><name ref="Onom.xml#Flavianus" nymRef="Φλαβιανός">Φλαβιανός</name></persName>,
                        <persName><name ref="Onom.xml#Eutyches" nymRef="Εὐτύχης">Εὐτύχης</name></persName>, <!--НЯМА ГО В АВТОРИТЕТНИЯ СПИСЪК-->
                    <lb n="5"/> <gap reason="lost" extent="unknown" unit="character"></gap><persName><name>ρος</name></persName>, <persName><name ref="Onom.xml#Ponponius" nymRef="Πωνπώνιος">Πωνπών<unclear>ιο</unclear>ς</name></persName>, <!--НЯМА ГО В АВТОРИТЕТНИЯ СПИСЪК-->
                        <persName><name ref="Onom.xml#Nikon" nymRef="Νίκων">Νίκων</name></persName>, <!--НЯМА ГО В АВТОРИТЕТНИЯ СПИСЪК-->
