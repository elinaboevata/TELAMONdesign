            <div type="edition" xml:space="preserve" xml:lang="grc">
                <ab>
                    <lb n="1"/><persName type="emperor" ref="listemperors#Caracalla">Ἀντωνεῖν<supplied reason="lost">ον</supplied></persName>. 
                    <lb n="2"/><w lemma="ἀγαθός">Ἀγαθῆι</w> <w lemma="τύχη">τύχηι</w>.
                    <lb n="3"/><w>Τὴν</w> <w lemma="ἀνάστασις">ἀνάστασιν</w>
                    <lb n="4"/> <w>κατὰ</w> <w lemma="δόγμα">δόγμα</w> <w lemma="βουλή">βου
                    <lb n="5" break="no"/>λῆς</w> <w>καὶ</w> <w lemma="δῆμος">δήμου</w> <placeName type="ethnic">Τραι
                    <lb n="6" break="no"/>ανέων</placeName> ἐπὶ <w lemma="συναρχία">συναρχί
                    <lb n="7" break="no"/>ας</w> <persName type="attested" ref="listPers#AT013"><name><expan><abbr>Αὐρ</abbr><ex>ηλίου</ex></expan></name> 
                    <name>Δημη<hi rend="ligature">τρ</hi>ίου</name> <persName type="attested" ref="listPers#AT024"><name>Απ
                        <lb n="8" break="no"/>φου</name></persName></persName> καὶ <w lemma="ἐπιμελέομαι">ἐπιμελουμέ
                        <lb n="9" break="no"/>νου</w> <w>τῆς</w> <w lemma="ἀνάστασις">ἀναστάσε
                        <lb n="10" break="no"/>ως</w> <w lemma="αὐτός">αὐτοῦ</w> <w>ἐκ</w> <w>τῶν</w> <w>τῆς</w>
                        <lb n="11"/><w lemma="πόλις">πόλεως</w>.
                </ab>
